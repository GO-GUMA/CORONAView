{
  "manifest_version":2,
  "name": "코로나 VIEW",
  "author" : "GO-GUMA",
  "description" : "코로나 VIEW는 대한민국의 확진자와 사용자가 선택한 특별시, 도의 코로나 확진자 정보를 표시합니다.",
  "version": "1.2.0",
  "version_name": "1.2.0",
  "browser_action": {
      "default_popup": "index.html",
      "default_icon" : "icon_128.png"
   },
   "icons" : {
      "128" : "icon_128.png"
   },
   "permissions": [
      "http://*/*", 
      "https://*/*", 
      "storage",
      "geolocation"
   ],
   "options_page": "options.html"
}